{
  "name": "haassie/core_seo",
  "description": "Core SEO functions for TYPO3",
  "type": "typo3-cms-extension",
  "license": "GPL-2.0",
  "keywords": [
    "TYPO3",
    "SEO"
  ],
  "authors": [
    {
      "name": "Richard Haeser",
      "role": "Developer",
      "email": "richardhaeser@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Haassie\\CoreSeo\\": "Classes/"
    }
  },
  "require": {
    "typo3/cms-core": "^8.7"
  },
  "replace": {
    "core_seo": "self.version",
    "typo3-ter/core-seo": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
